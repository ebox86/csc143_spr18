package StanleysStorage;

import java.util.Calendar;
import java.util.Date;

import StanleysStorage.Unit.unitType;

public class MyStorage {

	public static void main(String[] Args) throws Exception {
		Customer cust1 = new Customer("Evan", "3049067652");
		System.out.println(cust1.toString());
		
		Unit unit = new Unit(10, 10, 10, 10.0, unitType.STANDARD);
		Date today = Calendar.getInstance().getTime();
		
		unit.rent(cust1, today, 100.0);
		cust1.charge(50.0);
		cust1.charge(25.0);
		//System.out.println(unit.toString());
		
		Location loc = new Location("WA23Issaquah");
		//System.out.println(loc.toString());
		
		loc.getUnit(10, 10).rent(cust1, today, 100.0);
		
		Unit[] untArr = loc.getEmptyUnits(unitType.STANDARD);
		//System.out.println(untArr.length);
		//System.out.println(loc.toString());
		
		
		
	}
}
