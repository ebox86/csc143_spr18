package StanleysStorage;

public class Location {

	private String name;
	private String state;
	private String city;
	private int locationNum;
	private int rentedUnitCount;
	private int row;
	private int col;
	private Unit[][] unitArr;
	
	
	public Location(String state, int locationNum, String city, int row, int col) throws Exception {
		super();
		this.locationNum = locationNum;
		this.state = state;
		this.city = city;
		this.name = name;
		this.rentedUnitCount = 0;
		this.row = row;
		this.col = col;
		this.unitArr = unitArr;
	}


	/*
	 * Getters and Setters
	 */
	
	public String getState() {
		return state;
	}

	public String getCity() {
		return city;
	}

	/*
	 * methods
	 */

	public String getName() {
		return name;
	}


	public int getRentedUnitCount() {
		return rentedUnitCount;
	}
	
	
	
}
